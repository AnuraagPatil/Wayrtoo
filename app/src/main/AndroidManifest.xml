<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wayrtoo.excursion">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".application.excursion"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomeScreenActivity" />
        <activity android:name=".activity.IntroductionActivity" />
        <activity android:name=".activity.ActivitiesAllListsActivity" />
        <activity android:name=".activity.ViewSingleMapDirectionActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".activity.ActivitiesDetailsActivity" />
        <activity android:name=".activity.ViewAllMapActivity" />
        <activity android:name=".activity.BookingPaymentActivity" />
        <activity android:name=".activity.SelectCityListActivity" />
        <activity android:name=".activity.FilterActivity" />
        <activity android:name=".activity.ProfileActivity" />
        <activity android:name=".activity.FavoriteActivity" />
        <activity android:name=".activity.BookingCalendarTimeActivity" />
        <activity android:name=".activity.Filternew">


        </activity>
    </application>

</manifest>